<%= simple_form_for(@admin_system_admin) do |f| %>
  <%= render 'shared/form_errors', object: @admin_system_admin %>

  <div class="form-inputs">
	  <%= f.input :email %>
	  <%= f.input :first_name %>
	  <%= f.input :last_name %>
	  <%= f.input :super_admin %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>

<% end %>
