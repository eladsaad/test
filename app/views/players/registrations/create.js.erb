<% if resource.persisted? %>
    window.loadRemoteScript = false;
    History.pushState({}, '', '<%= response['Location'] %>');
    $.getScript('<%= response['Location'] %>');
<% else %>
    $('#error_messages').html('<%= escape_javascript(devise_error_messages!) %>');
<% end %>

$('#flash_messages').html('<%= escape_javascript(render partial: "shared/flash_messages", flash: flash) %>');